@model CoverType


<form method="post">
    <div class="border mt-4 p-3">
        <div class="mb-3">
            <h2 class="text-primary">Create Cover Type</h2>
            <hr />
        </div>

        <input type="hidden" asp-for="Id" />
        <div class="mb-3">
            <label asp-for="Name"></label>
            <input class="form-control" asp-for="Name" disabled/>
        </div>

        <button class="btn btn-danger" type="submit" style="width:150px;"> Delete </button>
        <a class="btn btn-secondary" asp-action="index"> Back to List </a>

    </div>
</form>

@section Scripts {
    @{
        <partial name="_ValidationScriptsPartial" />
    }
}